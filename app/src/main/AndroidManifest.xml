<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.helloworldapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".activity.myApplication"
        android:allowBackup="true"
        android:icon="@mipmap/dabo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.DiyAssembleActivity"></activity>
        <activity android:name=".activity.PerformanceAssembleActivity" />
        <activity android:name=".activity.AssembleListActivity" />
        <activity android:name=".activity.PriceAssembleActivity" />
        <activity android:name=".activity.ToolActivity" />
        <activity android:name=".activity.PasswordActivity" />
        <activity android:name=".activity.SafeActivity" />
        <activity android:name=".activity.SettingActivity" />
        <activity android:name=".activity.UserActivity" />
        <activity android:name=".activity.ContentActivity" />
        <activity android:name=".activity.TabHostActivity" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.HomeActivity" />
        <activity
            android:name=".activity.LuncherActivity"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>